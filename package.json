{
  "name": "nestjs-seed",
  "version": "0.1.0",
  "description": "NestJs Seed",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run lint && npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": " npm run lint && cross-env NODE_ENV=dev nest start --watch",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:docker": "npm run lint && cross-env NODE_ENV=docker nest start --watch",
    "start:hmr": "npm run lint && cross-env NODE_ENV=local nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npm run lint && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watchAll --maxWorkers=1",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:seed": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/migration/typeorm-seed-datasource.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/migration/typeorm-datasource.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:seed-generate": "npm run migration:seed migration:generate",
    "migration:seed-run": "npm run migration:seed migration:run",
    "migration:seed-revert": "npm run migration:seed migration:revert",
    "sonar": "node sonar-project.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/csurf": "^1.9.36",
    "@types/express-session": "^1.17.0",
    "@types/morgan": "^1.9.4",
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.0",
    "helmet": "^6.0.1",
    "jest-when": "^3.5.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nestjs-paginate": "^4.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rotating-file-stream": "^3.0.4",
    "rxjs": "^7.8.0",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jest-when": "^3.5.2",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^9.0.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "run-script-webpack-plugin": "^0.1.1",
    "sonarqube-scanner": "^3.0.0",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
